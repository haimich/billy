{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "exclude": [
    "node_modules",
    "static",
    "sql",
    "concept"
  ]
}
